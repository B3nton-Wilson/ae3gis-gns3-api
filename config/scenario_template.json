{
  "gns3_server_ip":"<GNS3_SERVER_IP>",
  "project_name": "<PROJECT_NAME>",
  "project_id": "<PROJECT_ID>",

  "templates": {
    "workstation": "<TEMPLATE_ID_WORKSTATION>",
    "openvswitch": "<TEMPLATE_ID_OPENVSWITCH>",
    "firewall": "<TEMPLATE_ID_IPTABLES>",
    "vpcs": "<TEMPLATE_ID_VPCS>"
  },

  "nodes": [
    {
      "name": "Workstation-1",
      "template_id": "<TEMPLATE_ID_WORKSTATION>",
      "start_script": "<path to start.sh>",
      "x": -300,
      "y": -250
    },
    {
      "name": "Workstation-2",
      "template_id": "<TEMPLATE_ID_WORKSTATION>",
      "x": 300,
      "y": -250
    },

    {
      "name": "OpenvSwitch-IT",
      "template_id": "<TEMPLATE_ID_OPENVSWITCH>",
      "x": 0,
      "y": -140
    },

    {
      "name": "IPTables-Firewall",
      "template_id": "<TEMPLATE_ID_IPTABLES>",
      "x": 0,
      "y": 0
    },

    {
      "name": "OpenvSwitch-OT",
      "template_id": "<TEMPLATE_ID_OPENVSWITCH>",
      "x": 0,
      "y": 150
    },

    {
      "name": "PLC-1",
      "template_id": "<TEMPLATE_ID_VPCS>",
      "x": -300,
      "y": 260
    },
    {
      "name": "PLC-2",
      "template_id": "<TEMPLATE_ID_VPCS>",
      "x": 300,
      "y": 260
    }
  ],

  "links": [
    {
      "nodes": [
        { "node_id": "NODE_WORKSTATION_1", "adapter_number": 0, "port_number": 0 },
        { "node_id": "NODE_OVS_IT",        "adapter_number": 0, "port_number": 0 }
      ]
    },
    {
      "link_id": "LINK_W2_TO_IT",
      "link_type": "ethernet",
      "nodes": [
        { "node_id": "NODE_WORKSTATION_2", "adapter_number": 0, "port_number": 0 },
        { "node_id": "NODE_OVS_IT",        "adapter_number": 0, "port_number": 1 }
      ]
    },
    {
      "nodes": [
        { "node_id": "NODE_OVS_IT",     "adapter_number": 0, "port_number": 2 },
        { "node_id": "NODE_FIREWALL",   "adapter_number": 0, "port_number": 0 }
      ]
    },
    {
      "nodes": [
        { "node_id": "NODE_FIREWALL",   "adapter_number": 1, "port_number": 0 },
        { "node_id": "NODE_OVS_OT",     "adapter_number": 0, "port_number": 2 }
      ]
    },
    {
      "nodes": [
        { "node_id": "NODE_PLC_1",  "adapter_number": 0, "port_number": 0 },
        { "node_id": "NODE_OVS_OT", "adapter_number": 0, "port_number": 0 }
      ]
    },
    {
      "nodes": [
        { "node_id": "NODE_PLC_2",  "adapter_number": 0, "port_number": 0 },
        { "node_id": "NODE_OVS_OT", "adapter_number": 0, "port_number": 1 }
      ]
    }
  ]
}
